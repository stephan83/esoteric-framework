		"pressedY": {
			"type":	"Number",
			"desc": "",
			"default": "0.0",
			"update": false
		}
		
	},
	
	"objectProperties": {
		"mouseEnabled": {
			"object": "interactiveObject",
			"property": "mouseEnabled",
			"type":	"Boolean",
			"desc": "Whether to enable mouse properties.",
			"default": "false",
			"update": false
		}
	}
}