{
	"desc": "The Viewport3D element is used to create a viewport.",
	
	"baseclass": "DisplayObjectElement",

	"imports": [
		"org.papervision3d.view.Viewport3D"
	],
	
	"properties": {
		"viewport3D": {
			"type":	"Viewport3D",
			"default": "new Viewport3D()",
			"desc": "The viewport 3D object.",
			"doc": false
		},
		"scene": {
			"type":	"Scene3DElement",
			"desc": "The Scene3DElement object."
		},
		"camera": {
			"type":	"CameraObject3DElement",
			"desc": "The CameraObject3DElement object."
		}
	},
	
	"objectProperties": {
		"autoClipping": {
			"object": "viewport3D",
			"property": "autoClipping",
			"type":	"Boolean",
			"default": "true",
			"desc": "Whether to enable auto clipping."
		},
		"viewportWidth": {
			"object": "viewport3D",
			"property": "viewportWidth",
			"type":	"Number",
			"default": "640.0",
			"desc": "The width of the viewport."
		},
		"viewportHeight": {
			"object": "viewport3D",
			"property": "viewportHeight",
			"type":	"Number",
			"default": "480.0",
			"desc": "The height of the viewport."
		},
		"autoCulling": {
			"object": "viewport3D",
			"property": "autoCulling",
			"type":	"Boolean",
			"default": "true",
			"desc": "Whether to enable auto culling."
		},
		"interactive": {
			"object": "viewport3D",
			"property": "interactive",
			"type":	"Boolean",
			"default": "true",
			"desc": "Whether the viewport is interactive."
		}
	}
	
}