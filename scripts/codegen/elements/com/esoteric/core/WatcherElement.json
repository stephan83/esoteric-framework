{
	"desc": "The WatcherListener element is used to listen to changes to an expression.",
	
	"baseclass": "ScriptElement",

	"imports": [
		"flash.events.Event"
	],
	
	"properties": {
		"watch": {
			"type":	"*",
			"desc": "The expression to watch."
		},
		"lastEvent": {
			"namespace" : "protected",
			"type":	"Event",
			"desc": "The last processed event.",
			"bindable": false,
			"update": false
		}
	}
}